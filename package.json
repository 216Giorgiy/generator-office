{
  "name": "generator-office",
  "version": "1.0.0-beta.1",
  "description": "",
  "repository": "/generator-office",
  "scripts": {
    "copy": "cpx \"src/app/templates/**/*.*\" generators/app/templates",
    "copy-watch": "cpx \"src/app/templates/**/*.*\" generators/app/templates --watch",
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "start": "rimraf generators && concurrently \"npm run tsc-watch\" \"npm run copy-watch\"",
    "build": "rimraf generators && concurrently \"npm run tsc\" \"npm run copy\"",
    "test": "mocha -r ts-node/register generators/test/**.js",
    "postinstall": "typings install"
  },
  "engines": {
    "node": ">6.9.0"
  },
  "homepage": "",
  "license": "MIT",
  "author": {
    "name": "",
    "email": "me@xyz.com",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "office",
    "yeoman-generator",
    "office-add-in",
    "add-in",
    "outlook",
    "excel",
    "word",
    "powerpoint",
    "onenote",
    "project"
  ],
  "dependencies": {
    "applicationinsights": "^0.17.1",
    "chalk": "^1.1.3",
    "lodash": "^4.17.2",
    "rimraf": "^2.5.4",
    "uuid": "^3.0.1",
    "yeoman-generator": "^1.0.1",
    "yosay": "^1.2.1"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "cpx": "^1.5.0",
    "mocha": "^3.2.0",
    "ts-node": "^2.0.0",
    "typescript": "^2.1.4",
    "typings": "^2.0.0",
    "yeoman-assert": "^2.2.2",
    "yeoman-test": "^1.6.0"
  }
}
